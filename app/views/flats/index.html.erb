<nav class="navbar navbar-light bg-light d-flex">
  <form class="form-inline">
    <%= form_tag(flats_path, method: :get) do %>
    <%= text_field_tag(:search, params[:search]) %>
    <%= submit_tag ("Search"),class: "btn btn-primary ml-3" %>
    <% end %>
    <%# <input class="form-control mr-sm-2" type="search" placeholder="type something" aria-label="Search">
    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button> %>
    <%= link_to "Add a Flat", new_flat_path, class: "btn btn-primary ml-3" %>
  </form>
</nav>
<h1>All our Flats (<%= Flat.count %>)</h1>

<div class="card-deck">
  <% @flats.each do |flat|  %>
  <div class="card" style="width: 18rem;">
    <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title"><%= flat.name %></h5>
      <p class="card-text"><%= flat.description %></p>
      <p class="card-text"><%= flat.address %></p>
      <p class="card-text"><%= flat.price_per_night %>💸</p>
      <p class="card-text"><%= flat.number_of_guests %>🧑🏽‍🤝‍🧑🏽</p>
    </div>
    <div class="card-footer">
      <%= link_to "More Info", flat_path(flat), class: "btn btn-primary" %>
      <%= link_to "Edit the flat", edit_flat_path(flat), class: "btn btn-primary"%>
      <%= link_to "Remove", flat_path(flat), method: :delete, class: "btn btn-danger", data: { confirm: "Are you sure?" }%>
    </div>
  </div>
  <% end %>
</div>
